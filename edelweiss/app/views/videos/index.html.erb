<h1>Videos</h1>
  <ul class="no-bullet">
    <% @videos.each do |video| %>
      <li><h2><%= video.name%></h2></li>
       <div class="flex-video">
        <iframe src="<%=video.url%>" frameborder="0" allowfullscreen></iframe>
        </div>
      <li><%= link_to 'Delete Video', video, method: :delete, data: { confirm: 'Are you sure?' }, class:"button" %></li>
      <%end%>
</ul>
<br>

<%= link_to 'New Video', new_video_path, class:"button" %>
