<% if session[:admin] %>
<div class="admin">
<h1>New Video</h1>
<%= form_for(@video,multipart: true) do |f| %>
    <%=f.text_field :name, label: false, placeholder:"Enter Video Name"%>
    <%= f.label "Upload Video:" %>
     <%=f.file_field :upload%>

     <%= f.label "Or" %>
     <br>
    <%=f.text_field :url, label: false, placeholder:"Enter Youtube URL"%>
    <%= f.submit 'Add Video', :class => 'tiny button round' %>
<% end %>
<%= link_to 'Back to Videos', videos_path, class: "jet" %>
</div>
<%end%>